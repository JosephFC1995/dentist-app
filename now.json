{
    "version": 2,
    "builds": [{
        "src": "nuxt.config.js",
        "use": "@nuxtjs/now-builder",
        "config": {
            "serverFiles": ["static/**"]
        }
    }],
    "routes": [{
            "src": "/_nuxt/.+",
            "headers": {
                "cache-control": "s-maxage=31536000"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": {
        "AXIOS_URL": "https://app-dent.josephfc.space/api"
    }
}